  "license": "MIT",
  "keywords": [
    "evaluation",
    "viewer",
    "fullstack",
    "react",
    "express"
  ],
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^8.36.0",
    "@typescript-eslint/parser": "^8.36.0",
    "concurrently": "^9.1.2",
    "eslint": "^9.30.1",
    "prettier": "^3.6.2"
  }
}
